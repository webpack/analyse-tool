{
  "name": "analyse-tool",
  "version": "1.0.0",
  "private": true,
  "description": "A tool to analyse your webpack build result. It allows to browse the compilation and points out options to optimize the build.",
  "main": "index.js",
  "engines": {
    "node": ">=6.8.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "build": "npm run dist:clean && npm run copy:static && npm run compile",
    "dist:clean": "rm -rf dist && mkdir dist",
    "copy:static": "cp -r src/static/* dist",
    "compile": "NODE_ENV=production webpack -p --bail",
    "dev": "node server",
    "dev:bs": "node server --with-browsersync",
    "flow": "flow check",
    "lint": "eslint . ./",
    "start": "npm run dev",
    "start:bs": "npm run dev:bs",
    "test": "npm run lint && npm run flow",
    "travis": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webpack/analyse-tool.git"
  },
  "keywords": [
    "webpack",
    "analyse"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webpack/analyse-tool/issues"
  },
  "homepage": "https://github.com/webpack/analyse-tool#readme",
  "dependencies": {
    "axios": "^0.15.1",
    "classnames": "^2.2.5",
    "empty": "^0.10.1",
    "lodash.merge": "^4.6.0",
    "react": "^15.4.0-rc.4",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.4.0-rc.4",
    "react-intl": "2.1.5",
    "react-redux": "^5.0.0-beta.3",
    "react-router": "^3.0.0-beta.1",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-form": "^5.3.3",
    "redux-form-validation": "^0.0.8",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sigma": "^1.1.0",
    "url": "^0.11.0"
  },
