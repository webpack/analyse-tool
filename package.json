{
  "name": "analyse-tool",
  "version": "1.0.0",
  "private": true,
  "description": "A tool to analyse your webpack build result. It allows to browse the compilation and points out options to optimize the build.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src test",
    "beautify-lint": "beautify-lint src/**.js test/**.js",
    "test": "npm run lint && cross-env NODE_ENV=test karma start --single-run",
    "travis": "karma start --browsers Firefox --single-run",
    "beautify": "beautify-rewrite src/**.js test/**.js",
    "build": "npm run lint && npm run beautify-lint && cross-env NODE_ENV=production webpack",
    "dev-server": "cross-env NODE_ENV=development node server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webpack/analyse-tool.git"
  },
  "keywords": [
    "webpack",
    "analyse"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webpack/analyse-tool/issues"
  },
  "homepage": "https://github.com/webpack/analyse-tool#readme",
  "dependencies": {
    "babel-core": "^5.8.34",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.6",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-addons-css-transition-group": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-transform-hmr": "^1.0.1",
    "react-toolbox": "^0.11.5",
    "redux": "^3.0.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.6"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "autoprefixer": "^6.1.1",
    "babel-eslint": "^4.1.5",
    "beautify-lint": "^1.0.4",
    "chai": "^3.4.1",
    "diff": "^2.2.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-nodeca": "^1.0.3",
    "eslint-plugin-react": "^3.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "glob": "^6.0.1",
    "html-webpack-plugin": "^1.7.0",
    "js-beautify": "^1.5.10",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.5.1"
  }
}
